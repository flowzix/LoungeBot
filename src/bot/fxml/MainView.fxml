<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-background-color 0xdfdfdfff?>

<GridPane alignment="CENTER" depthTest="INHERIT" gridLinesVisible="false" hgap="10.0" minHeight="500.0" minWidth="700.0" prefHeight="600.0" prefWidth="800.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="bot.controller.Controller">
  <children>
    <Label minWidth="-Infinity" prefHeight="-1.0" prefWidth="-1.0" text="Login" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Label text="Hasło" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <TextField fx:id="inputLogin" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
      <GridPane.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
      </GridPane.margin>
    </TextField>
    <Button fx:id="loginButton" mnemonicParsing="false" onMouseClicked="#onLoginClicked" prefWidth="97.0" text="Zaloguj" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
    <VBox id="VBox" fx:id="loginStatusBox" alignment="CENTER" spacing="5.0" style="" GridPane.columnIndex="0" GridPane.rowIndex="2">
      <children>
        <Label id="loggedInLabel" fx:id="loginStatusLabel" alignment="CENTER" contentDisplay="CENTER" prefWidth="66.0" style="-fx-background-color: rgba(255, 255, 255, 0.5);" text="STATUS" textAlignment="CENTER" />
      </children>
    </VBox>
    <Label text="Rzeczy" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <TextArea fx:id="logArea" prefWidth="200.0" wrapText="true" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="5">
      <GridPane.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
      </GridPane.margin>
    </TextArea>
    <Label text="Postępy" GridPane.columnIndex="0" GridPane.margin="$x1" GridPane.rowIndex="5" GridPane.valignment="TOP" />
    <Button fx:id="startButton" mnemonicParsing="false" onMouseClicked="#onStartBotClicked" text="Start" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="2">
      <GridPane.margin>
        <Insets />
      </GridPane.margin>
    </Button>
    <Button fx:id="stopButton" mnemonicParsing="false" text="Stop" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
    <TableView fx:id="itemsTable" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.margin="$x1" GridPane.rowIndex="3" GridPane.rowSpan="2">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="301.0" text="Słowa kluczowe" fx:id="columnKeywords" />
        <TableColumn id="" maxWidth="5000.0" minWidth="10.0" prefWidth="81.0" text="Rozmiar" fx:id="columnSize" />
        <TableColumn id="" maxWidth="5000.0" minWidth="10.0" prefWidth="106.0" text="Dow. Rozm." fx:id="columnAnySize" />
        <TableColumn id="" maxWidth="5000.0" minWidth="10.0" prefWidth="171.0" text="Maks. cena" fx:id="columnMaxPrice" />
        <TableColumn id="" maxWidth="5000.0" minWidth="10.0" prefWidth="171.0" text="Kampania" fx:id="columnCampaignID" />
      </columns>
    </TableView>
    <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="4">
      <children>
        <Button fx:id="addItemButton" alignment="CENTER" contentDisplay="TOP" minWidth="-1.0" mnemonicParsing="false" onMouseClicked="#onAddItemClicked" prefWidth="83.0" text="Dodaj" textAlignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0" />
        <Button fx:id="removeItemButton" mnemonicParsing="false" onMouseClicked="#onRemoveItemClicked" prefWidth="83.0" text="Usuń" textAlignment="LEFT" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="35.0" />
      </children>
    </AnchorPane>
    <PasswordField fx:id="inputPassword" prefWidth="200.0" GridPane.columnIndex="1" GridPane.margin="$x1" GridPane.rowIndex="1" />
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="192.0" minWidth="10.0" prefWidth="88.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="311.0" minWidth="10.0" prefWidth="290.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="192.0" minWidth="10.0" prefWidth="188.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="192.0" minWidth="10.0" prefWidth="189.0" />
  </columnConstraints>
  <padding>
    <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
  </padding>
  <rowConstraints>
    <RowConstraints maxHeight="200.0" minHeight="10.0" prefHeight="31.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="305.0" minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="265.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="343.0" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="360.0" minHeight="10.0" prefHeight="271.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="189.0" minHeight="10.0" prefHeight="144.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
